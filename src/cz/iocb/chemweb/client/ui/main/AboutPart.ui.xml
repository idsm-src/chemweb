<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .about {
      min-width: 500px;
	}
    
    .page {
      padding: 15px;
    }

    .section {
      margin-bottom: 15px;
      border: 1px solid #81b0d4;
      border-collapse: separate;
      border-radius: 5px;
      overflow: hidden;
    }

    .section div {
      padding: 5px;
    }

    .header {
      font-weight: bold;
      padding-bottom: 7px;
      border-bottom: 1px solid #81b0d4;
      background: #e3eaef;
    }
    
    .about ul {
      margin-top: 0.5em;
      padding-left: 25px;
    }

    .about ul li {
      margin: 0 0 3px 0;
    }
    
    .statistics {
      border-spacing: 0;
      border-collapse: collapse;
      margin: 5px 15px;
    }
    
    .statistics th {
	  text-align: left;
	  padding: 4px 8px;
	  border-bottom: 1px solid #81b0d4 !important;
	}
	
	.statistics td {
      border-top: 1px solid #eee;
      padding: 4px 8px;
    }
    
    .statistics td:first-child {
      padding-right: 50px;
    }
    
    .statistics td:last-child {
      text-align: right;
    }
	
  </ui:style>

