#set($BAO = $entity.toString().replaceAll(".*/", "").replaceAll(">",""))

#sparql($title)
  SELECT ?TITLE FROM pubchem:bioassay WHERE
  {
    $entity dcterms:title ?TITLE
  }
#end

<table style="width:100%">
  <tbody>
    <tr><td>
      <b>$BAO</b>
    </td></tr>
    #if($title.TITLE)
    <tr><td>
      #escapeHTML($title.TITLE)
    </td></tr>
    #end
  </tbody>
</table>
