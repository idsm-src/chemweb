#set($PMID = $entity.getValue().replaceAll(".*/", ""))

#sparql($title)
  SELECT ?TITLE FROM pubchem:reference WHERE
  {
    $entity dcterms:title ?TITLE
  }
#end

<table style="width:100%">
  <tbody>
    <tr>
      <td><b>$PMID</b></td>
    </tr>
    #if($title.TITLE)
    <tr>
      <td>#escapeHTML($title.TITLE)</td>
    </tr>
    #end
  </tbody>
</table>
