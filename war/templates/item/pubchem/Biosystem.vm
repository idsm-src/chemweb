#set($BSID = $entity.toString().replaceAll(".*/", "").replaceAll(">",""))

#sparql($title)
  SELECT ?TITLE FROM pubchem:biosystem WHERE
  {
    $entity dcterms:title ?TITLE
  }
#end

<table style="width:100%">
  <tbody>
    <tr><td>
      <b>$BSID</b>
    </td></tr>
    #if($title.TITLE)
    <tr><td>
      #escapeHTML($title.TITLE)
    </td></tr>
    #end
  </tbody>
</table>
