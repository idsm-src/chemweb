#sparql($synonym)
  SELECT ?VALUE FROM pubchem:synonym WHERE
  {
    $entity sio:SIO_000300 ?VALUE
  }
#end

<table style="width:100%">
  <tbody>
    <tr>
      <td>#escapeHTML($synonym.VALUE)</td>
    </tr>
    <tr>
      <td><small style="color:gray">#escapeIRI($entity)<small></td>
    </tr>
  </tbody>
</table>
